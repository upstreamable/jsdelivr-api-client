{
  "name": "upstreamable/jsdelivr-api-client",
  "description": "Client for the jsDelivr data API https://github.com/jsdelivr/data.jsdelivr.com",
  "license": "GPL-3.0-or-later",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Rodrigo Aguilera",
      "homepage": "https://www.rodrigoaguilera.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "Upstreamable\\JsdelivrApiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Upstreamable\\JsdelivrApiClient\\tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.2",
    "psr/http-message": "^1.0",
    "php-http/httplug": "^2.0",
    "php-http/message": "^1.7",
    "php-http/discovery": "^1.6",
    "php-http/message-factory": "^v1.0",
    "php-http/client-implementation": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "phpunit/phpunit": "^8",
    "symfony/yaml": "^4.2",
    "donatj/mock-webserver": "^2.0",
    "php-http/guzzle6-adapter": "^2.0"
  },
  "suggest": {
    "php-http/guzzle6-adapter": "In order to use Guzzle v6 as the HTTP client"
  }
}
